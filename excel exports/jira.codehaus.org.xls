			{<br/>
				@Override<br/>
				public List&lt;String&gt; call()<br/>
				{<br/>
					return new ArrayList&lt;String&gt;();<br/>
				}<br/>
			});<br/>
	}<br/>
<br/>
	public List&lt;String&gt; test2()<br/>
	{<br/>
		TestStaticJ t = new TestStaticJ();<br/>
		return t.doSomething(new Closure&lt;List&lt;String&gt;&gt;(this)<br/>
			{<br/>
				@Override<br/>
				public List&lt;String&gt; call()<br/>
				{<br/>
