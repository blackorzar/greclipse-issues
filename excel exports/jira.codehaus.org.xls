<br/>
List&lt;String&gt; strings28 = [&#39;1x&#39;, &#39;2xx&#39;, &#39;3xxx&#39;, &#39;4xxxx&#39;];    // 28 chars between []<br/>
List&lt;String&gt; strings29 = [<br/>
